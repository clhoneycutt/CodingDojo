@model SurveyForm
<div class="outline offset-3 col-6">
    <h4>Submitted Info</h4>
    <table>
        <tbody>
            <tr>
                <td>Name:</td>
                <td>@Model.Name</td>
            </tr>
            <tr>
                <td>Dojo Location:</td>
                <td>@Model.Dojo</td>
            </tr>
            <tr>
                <td>Favorite Language:</td>
                <td>@Model.Language</td>
            </tr>
            <tr>
                <td>Comment:</td>
                <td>@Model.Comment</td>
            </tr>
        </tbody>
    </table>
    <div class="text-right">
        <a href="/" class="btn btn-outline-dark">Go Back</a>
    </div>
</div>